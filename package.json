{
  "name": "react-homework",
  "version": "1.0.0",
  "description": "",
  "main": "src",
  "scripts": {
    "client:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config ./webpack/webpack.client.config.js --port 8080 --colors ",
    "client:build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.client.config.js",
    "server:dev": "cross-env NODE_ENV=development webpack --watch --config ./webpack/webpack.server.config.js --colors",
    "server:build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.server.config.js --colors",
    "server:watch": "cross-env NODE_ENV=development nodemon server.js --watch",
    "server": "cross-env NODE_ENV=production node server.js",
    "watch": "npm-run-all --parallel client:dev server:dev server:watch",
    "start": "npm-run-all --serial server:build watch",
    "start:server-dev": "nodemon --watch src",
    "start:webpack-dev": "node webpack/webpack-dev-server",
    "dev": "npm-run-all --parallel start:server-dev start:webpack-dev",
    "test": "jest",
    "format": "prettier --write --single-quote --trailing-comma=all src/**/*.js",
    "precommit": "lint-staged",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazurevich/react-homework.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mazurevich/react-homework/issues"
  },
  "homepage": "https://github.com/mazurevich/react-homework#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "lint-staged": "^4.1.0",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "styled-components": "^2.1.2",
    "webpack-isomorphic-tools": "^3.0.3",
    "webpack-md5-hash": "0.0.5"
  }
}
